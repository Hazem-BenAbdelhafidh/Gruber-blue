{
    "name": "Gruber Blue",
    "type": "dark",
    "semanticHighlighting": true,
    "tokenColors": [
        {
            "name": "Comments",
            "scope": "comment, punctuation.definition.comment",
            "settings": {
                "foreground": "#7d8fa3"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#4cd84c"
            }
        },
        {
            "name": "String Punctuation",
            "scope": "punctuation.definition.string.begin,punctuation.definition.string.end",
            "settings": {
                "foreground": "#4cd84c"
            }
        },
        {
            "name": "Escape Characters",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#4cd84c"
            }
        },
        {
            "name": "Numbers",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#ff5500"
            }
        },
        {
            "name": "Keywords",
            "scope": "keyword",
            "settings": {
                "foreground": "#00A6FF"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "foreground": "#00A6FF"
            }
        },
        {
            "name": "Keyword Control",
            "scope": "keyword.control",
            "settings": {
                "foreground": "#00A6FF"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#e6e9f0"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "meta.require",
                "support.function.any-method",
                "variable.function",
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Classes",
            "scope": "support.class, entity.name.type.class, entity.name.class",
            "settings": {
                "foreground": "#b8c2e0"
            }
        },
        {
            "name": "Types",
            "scope": "entity.name.type, support.type",
            "settings": {
                "foreground": "#b8c2e0"
            }
        },
        {
            "name": "Variables",
            "scope": "variable",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Language Variables",
            "scope": "variable.language",
            "settings": {
                "foreground": "#b8c2e0"
            }
        },
        {
            "name": "Constants",
            "scope": "constant",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Tags",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#00A6FF"
            }
        },
        {
            "name": "JSX Components",
            "scope": "support.class.component",
            "settings": {
                "foreground": "#66ccff"
            }
        },
        {
            "name": "Attributes",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#e6e9f0"
            }
        },
        {
            "name": "Boolean",
            "scope": "constant.language.bool",
            "settings": {
                "foreground": "#b8c2e0"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation.separator.delimiter, punctuation.definition, punctuation.section",
            "settings": {
                "foreground": "#9ca7c2"
            }
        },
        {
            "name": "Parentheses and Brackets",
            "scope": "punctuation.section.parens, punctuation.section.brackets",
            "settings": {
                "foreground": "#9ca7c2"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Markdown Headings",
            "scope": "markup.heading, entity.name.section.markdown",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Markdown Links",
            "scope": "markup.underline.link.markdown",
            "settings": {
                "foreground": "#00A6FF"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Italic",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid.illegal, invalid.deprecated",
            "settings": {
                "foreground": "#ff3333"
            }
        }
    ],
    "colors": {
        "foreground": "#ffffff",
        "focusBorder": "#333333",
        "selection.background": "#333333",
        "activityBar.background": "#1A1A1A",
        "activityBar.foreground": "#00A6FF",
        "activityBar.inactiveForeground": "#7d8fa3",
        "sideBar.background": "#1A1A1A",
        "sideBar.foreground": "#ffffff",
        "sideBarSectionHeader.background": "#1A1A1A",
        "sideBarSectionHeader.foreground": "#ffffff",
        "editor.background": "#1A1A1A",
        "editor.foreground": "#ffffff",
        "editorLineNumber.foreground": "#7d8fa3",
        "editorLineNumber.activeForeground": "#00A6FF",
        "editorCursor.foreground": "#00A6FF",
        "editor.selectionBackground": "#333333",
        "editor.lineHighlightBackground": "#252525",
        "editor.findMatchBackground": "#333333",
        "editor.findMatchHighlightBackground": "#33333300",
        "editorBracketMatch.background": "#333333",
        "editorBracketMatch.border": "#333333",
        "statusBar.background": "#151515",
        "statusBar.foreground": "#ffffff",
        "statusBarItem.hoverBackground": "#333333",
        "titleBar.activeBackground": "#1A1A1A",
        "titleBar.activeForeground": "#ffffff",
        "tab.activeBackground": "#1A1A1A",
        "tab.activeForeground": "#ffffff",
        "tab.inactiveBackground": "#1E1E1E",
        "tab.inactiveForeground": "#7d8fa3",
        "tab.activeBorder": "#00A6FF",
        "input.background": "#252525",
        "input.foreground": "#ffffff",
        "input.placeholderForeground": "#7d8fa3",
        "button.background": "#333333",
        "button.foreground": "#ffffff",
        "dropdown.background": "#252525",
        "dropdown.foreground": "#ffffff",
        "list.activeSelectionBackground": "#333333",
        "list.activeSelectionForeground": "#ffffff",
        "list.hoverBackground": "#252525",
        "list.hoverForeground": "#ffffff",
        "panel.background": "#161616",
        "panel.border": "#1A1A1A",
        "editorSuggestWidget.background": "#252525",
        "editorSuggestWidget.foreground": "#ffffff",
        "editorHoverWidget.background": "#252525",
        "editorHoverWidget.foreground": "#ffffff",
        "minimap.background": "#1A1A1A",
        "scrollbarSlider.background": "#333333",
        "scrollbarSlider.hoverBackground": "#444444",
        "badge.background": "#00A6FF",
        "badge.foreground": "#FFFFFF",
        "terminal.foreground": "#ffffff",
        "terminal.background": "#1A1A1A",
        "terminalCursor.foreground": "#00A6FF",
        "terminal.ansiBlack": "#000000",
        "terminal.ansiBlue": "#00A6FF",
        "terminal.ansiBrightBlack": "#666666",
        "terminal.ansiBrightBlue": "#00A6FF",
        "terminal.ansiBrightCyan": "#29B8DB",
        "terminal.ansiBrightGreen": "#4cd84c",
        "terminal.ansiBrightMagenta": "#D670D6",
        "terminal.ansiBrightRed": "#ff3333",
        "terminal.ansiBrightWhite": "#ffffff",
        "terminal.ansiBrightYellow": "#ffd700",
        "terminal.ansiCyan": "#11A8CD",
        "terminal.ansiGreen": "#4cd84c",
        "terminal.ansiMagenta": "#BC3FBC",
        "terminal.ansiRed": "#ff3333",
        "terminal.ansiWhite": "#ffffff",
        "terminal.ansiYellow": "#ffd700"
    }
}